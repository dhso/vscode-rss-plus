{
    "name": "rss-plus",
    "displayName": "RSS Plus",
    "description": "An RSS reader embedded in Visual Studio Code",
    "license": "MIT",
    "icon": "logo.png",
    "version": "0.10.4",
    "publisher": "dhso",
    "author": "luyuhuang & dhso",
    "homepage": "https://github.com/luyuhuang/vscode-rss.git",
    "repository": {
        "type": "git",
        "url": "https://github.com/luyuhuang/vscode-rss.git"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "news",
        "rss",
        "feed",
        "reader"
    ],
    "activationEvents": [
        "onView:rss-plus-feeds",
        "onView:rss-plus-articles"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "RSS Plus",
            "properties": {
                "rss-plus.accounts": {
                    "type": "object",
                    "default": {},
                    "description": "Feed accounts"
                },
                "rss-plus.interval": {
                    "type": "integer",
                    "default": 3600,
                    "description": "Refresh interval(s)"
                },
                "rss-plus.timeout": {
                    "type": "integer",
                    "default": 15,
                    "description": "Request timeout(s)"
                },
                "rss-plus.retry": {
                    "type": "integer",
                    "default": 1,
                    "description": "Request retries"
                },
                "rss-plus.fetch-unread-only": {
                    "type": "boolean",
                    "default": false,
                    "description": "Fetch unread articles only, for TTRSS and Inoreader"
                },
                "rss-plus.status-bar-length": {
                    "type": "number",
                    "default": 20,
                    "description": "Max length displayed in status bar"
                },
                "rss-plus.status-bar-notify": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether to show notification in status bar"
                },
                "rss-plus.status-bar-update": {
                    "type": "number",
                    "default": 5,
                    "description": "Notification update interval(s)"
                },
                "rss-plus.storage-path": {
                    "type": "string",
                    "default": null,
                    "description": "Data storage path"
                },
                "rss-plus.inoreader-domain": {
                    "type": "string",
                    "default": "www.inoreader.com",
                    "description": "Domain of Inoreader"
                },
                "rss-plus.inoreader-limit": {
                    "type": "integer",
                    "default": 100,
                    "minimum": 1,
                    "maximum": 1000,
                    "description": "Limit of the number of articles fetched by Inoreader at a time"
                }
            }
        },
        "commands": [
            {
                "command": "rss-plus.select",
                "title": "Select"
            },
            {
                "command": "rss-plus.new-account",
                "title": "New account",
                "icon": "$(add)"
            },
            {
                "command": "rss-plus.del-account",
                "title": "Delete"
            },
            {
                "command": "rss-plus.account-rename",
                "title": "Rename"
            },
            {
                "command": "rss-plus.account-modify",
                "title": "Modify"
            },
            {
                "command": "rss-plus.articles",
                "title": "Articles"
            },
            {
                "command": "rss-plus.read",
                "title": "Read"
            },
            {
                "command": "rss-plus.read-notification",
                "title": "Read from notification"
            },
            {
                "command": "rss-plus.refresh",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "rss-plus.refresh-account",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "rss-plus.refresh-one",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "rss-plus.open-website",
                "title": "Open website"
            },
            {
                "command": "rss-plus.open-link",
                "title": "Open link",
                "icon": "$(globe)"
            },
            {
                "command": "rss-plus.mark-read",
                "title": "Mark as read",
                "icon": "$(check)"
            },
            {
                "command": "rss-plus.mark-unread",
                "title": "Mark as unread"
            },
            {
                "command": "rss-plus.mark-all-read",
                "title": "Mark all as read",
                "icon": "$(check)"
            },
            {
                "command": "rss-plus.mark-account-read",
                "title": "Mark all as read",
                "icon": "$(check)"
            },
            {
                "command": "rss-plus.add-feed",
                "title": "Add feed",
                "icon": "$(add)"
            },
            {
                "command": "rss-plus.remove-feed",
                "title": "Remove"
            },
            {
                "command": "rss-plus.add-to-favorites",
                "title": "Add to favorites",
                "icon": "$(star-empty)"
            },
            {
                "command": "rss-plus.remove-from-favorites",
                "title": "Remove from favorites"
            },
            {
                "command": "rss-plus.export-to-opml",
                "title": "Export to OPML"
            },
            {
                "command": "rss-plus.import-from-opml",
                "title": "Import from OPML"
            },
            {
                "command": "rss-plus.clean-old-articles",
                "title": "Clean old articles"
            },
            {
                "command": "rss-plus.clean-all-old-articles",
                "title": "Clean old articles"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "rss-plus-reader",
                    "title": "RSS Reader Plus",
                    "icon": "resources/rss.svg"
                }
            ]
        },
        "views": {
            "rss-plus-reader": [
                {
                    "id": "rss-plus-accounts",
                    "name": "Accounts"
                },
                {
                    "id": "rss-plus-feeds",
                    "name": "Feeds"
                },
                {
                    "id": "rss-plus-articles",
                    "name": "Articles"
                },
                {
                    "id": "rss-plus-favorites",
                    "name": "Favorites"
                }
            ]
        },
        "menus": {
            "commandPalette": [
                {
                    "command": "rss-plus.select",
                    "when": "false"
                },
                {
                    "command": "rss-plus.articles",
                    "when": "false"
                },
                {
                    "command": "rss-plus.read",
                    "when": "false"
                },
                {
                    "command": "rss-plus.mark-read",
                    "when": "false"
                },
                {
                    "command": "rss-plus.mark-unread",
                    "when": "false"
                },
                {
                    "command": "rss-plus.mark-all-read",
                    "when": "false"
                },
                {
                    "command": "rss-plus.mark-account-read",
                    "when": "false"
                },
                {
                    "command": "rss-plus.refresh",
                    "when": "false"
                },
                {
                    "command": "rss-plus.refresh-account",
                    "when": "false"
                },
                {
                    "command": "rss-plus.refresh-one",
                    "when": "false"
                },
                {
                    "command": "rss-plus.open-website",
                    "when": "false"
                },
                {
                    "command": "rss-plus.open-link",
                    "when": "false"
                },
                {
                    "command": "rss-plus.add-feed",
                    "when": "false"
                },
                {
                    "command": "rss-plus.remove-feed",
                    "when": "false"
                },
                {
                    "command": "rss-plus.add-to-favorites",
                    "when": "false"
                },
                {
                    "command": "rss-plus.remove-from-favorites",
                    "when": "false"
                },
                {
                    "command": "rss-plus.new-account",
                    "when": "false"
                },
                {
                    "command": "rss-plus.del-account",
                    "when": "false"
                },
                {
                    "command": "rss-plus.account-rename",
                    "when": "false"
                },
                {
                    "command": "rss-plus.account-modify",
                    "when": "false"
                },
                {
                    "command": "rss-plus.export-to-opml",
                    "when": "false"
                },
                {
                    "command": "rss-plus.import-from-opml",
                    "when": "false"
                },
                {
                    "command": "rss-plus.clean-old-articles",
                    "when": "false"
                },
                {
                    "command": "rss-plus.clean-all-old-articles",
                    "when": "false"
                }
            ],
            "view/title": [
                {
                    "command": "rss-plus.refresh",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.new-account",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.refresh-account",
                    "when": "view == rss-plus-feeds",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.add-feed",
                    "when": "view == rss-plus-feeds",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.mark-account-read",
                    "when": "view == rss-plus-feeds",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.refresh-one",
                    "when": "view == rss-plus-articles",
                    "group": "navigation"
                },
                {
                    "command": "rss-plus.mark-all-read",
                    "when": "view == rss-plus-articles",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "rss-plus.refresh-account",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation@1"
                },
                {
                    "command": "rss-plus.mark-account-read",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation@2"
                },
                {
                    "command": "rss-plus.account-rename",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation@3"
                },
                {
                    "command": "rss-plus.account-modify",
                    "when": "view == rss-plus-accounts && viewItem != local",
                    "group": "navigation@4"
                },
                {
                    "command": "rss-plus.export-to-opml",
                    "when": "view == rss-plus-accounts && viewItem == local",
                    "group": "navigation@5"
                },
                {
                    "command": "rss-plus.import-from-opml",
                    "when": "view == rss-plus-accounts && viewItem == local",
                    "group": "navigation@6"
                },
                {
                    "command": "rss-plus.clean-all-old-articles",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation@8"
                },
                {
                    "command": "rss-plus.del-account",
                    "when": "view == rss-plus-accounts",
                    "group": "navigation@9"
                },
                {
                    "command": "rss-plus.open-link",
                    "when": "viewItem == article",
                    "group": "inline"
                },
                {
                    "command": "rss-plus.mark-read",
                    "when": "view == rss-plus-articles",
                    "group": "inline"
                },
                {
                    "command": "rss-plus.mark-unread",
                    "when": "view == rss-plus-articles"
                },
                {
                    "command": "rss-plus.add-to-favorites",
                    "when": "view == rss-plus-articles",
                    "group": "inline"
                },
                {
                    "command": "rss-plus.remove-from-favorites",
                    "when": "view == rss-plus-favorites && viewItem == article"
                },
                {
                    "command": "rss-plus.refresh-one",
                    "when": "viewItem == feed",
                    "group": "navigation@1"
                },
                {
                    "command": "rss-plus.mark-all-read",
                    "when": "viewItem == feed",
                    "group": "navigation@2"
                },
                {
                    "command": "rss-plus.open-website",
                    "when": "viewItem == feed",
                    "group": "navigation@3"
                },
                {
                    "command": "rss-plus.clean-old-articles",
                    "when": "viewItem == feed",
                    "group": "navigation@4"
                },
                {
                    "command": "rss-plus.remove-feed",
                    "when": "viewItem == feed",
                    "group": "navigation@5"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile-release",
        "compile-release": "rm -rf ./out && webpack --mode production",
        "compile": "rm -rf ./out && tsc -p ./",
        "lint": "eslint src --ext ts",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.1",
        "@types/glob": "^7.1.1",
        "@types/he": "^1.1.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^13.11.0",
        "@types/uuid": "^7.0.3",
        "@types/vscode": "^1.40.0",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "eslint": "^7.32.0",
        "glob": "^7.1.6",
        "mocha": "^9.2.0",
        "ts-loader": "^7.0.5",
        "typescript": "^4.2.4",
        "vscode-test": "^1.3.0",
        "webpack": "^5.76.0",
        "webpack-cli": "^4.8.0"
    },
    "dependencies": {
        "cheerio": "1.0.0-rc.10",
        "fast-xml-parser": "^4.4.1",
        "fs-extra": "^9.0.1",
        "got": "11.8.3",
        "he": "^1.2.0",
        "iconv-lite": "^0.5.1",
        "uuid": "^8.0.0"
    }
}
